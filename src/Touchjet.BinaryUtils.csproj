<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Authors>Zhen Liu</Authors>
    <PackageLicenseUrl>https://github.com/touchjet/BinaryUtils/blob/master/LICENSE</PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/touchjet/BinaryUtils</PackageProjectUrl>
    <Description>C# utilities handling binaries.</Description>
    <PackageId>Touchjet.BinaryUtils</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <PackOnBuild>true</PackOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2018.2.1" />
    <PackageReference Include="System.Diagnostics.Contracts" Version="4.3.0" />
    <PackageReference Include="NuGet.Build.Packaging" Version="0.2.0" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Logging\ArraySegmentLoggingExtensions.cs" />
    <Compile Remove="ArraySegmentLoggingExtensions.cs" />
  </ItemGroup>
</Project>
